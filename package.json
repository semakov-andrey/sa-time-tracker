{
  "name": "time-tracker",
  "shortName": "time-tracker",
  "version": "1.0.0",
  "description": "time-tracker-app",
  "themeColor": "#000000",
  "scripts": {
    "install-deps": "yarn",
    "check-deps": "npm-check-updates",
    "update-deps": "npm-check-updates -u",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "start": "node --experimental-json-modules --no-warnings ./compiler/scripts/dev-server.js",
    "build": "node --experimental-json-modules --no-warnings ./compiler/scripts/build.js",
    "analyze-bundle": "webpack-bundle-analyzer build/stats.json",
    "stylelint": "stylelint './src/**/*.css' -f verbose",
    "stylelint-fix": "stylelint './src/**/*.css' -f verbose --fix",
    "eslint": "eslint './src/**/*.{ts,tsx}'",
    "eslint-fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "eslint-all": "eslint './{compiler,src}/**/*.{cjs,js,jsx,ts,tsx}' --no-error-on-unmatched-pattern --quiet",
    "eslint-all-fix": "eslint './{compiler,src}/**/*.{cjs,js,jsx,ts,tsx}' --no-error-on-unmatched-pattern --quiet --fix"
  },
  "keywords": [
    "time-tracker"
  ],
  "author": "Semakov Andrey",
  "type": "module",
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-hot-loader": "^4.1.1",
    "@types/uuid": "^8.3.1",
    "autoprefixer": "^10.3.1",
    "camelcase": "^6.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.1.0",
    "del": "^6.0.0",
    "eazy-logger": "^3.1.0",
    "esbuild-loader": "^2.13.1",
    "fast-glob": "^3.2.7",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "html-webpack-plugin": "^5.3.2",
    "image-size": "^1.0.0",
    "mini-css-extract-plugin": "^2.1.0",
    "normalize.css": "^8.0.1",
    "npm-check-updates": "^11.8.3",
    "postcss": "^8.3.5",
    "postcss-custom-media": "^8.0.0",
    "postcss-loader": "^6.1.1",
    "postcss-modules": "^4.1.3",
    "postcss-nested": "^5.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "redux": "^4.1.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "webpack": "^5.45.1",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "chokidar": "^3.5.2",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "^7.31.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-rulesdir": "^0.2.0",
    "express": "^4.17.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "log-update": "^4.0.0",
    "module-alias": "^2.2.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-order": "^4.1.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "config": {
    "devPort": 80,
    "entries": {
      "index": "index"
    },
    "directories": {
      "source": "src",
      "development": "tmp",
      "production": "build",
      "assets": "assets"
    }
  },
  "browserslist": [
    "> 0.5%"
  ],
  "_moduleAliases": {
    "eslint-plugin-rulesdir": "./compiler/eslint/rulesdir.cjs"
  },
  "lint-staged": {
    "src/**/*.css": "stylelint --fix",
    "src/**/*.{ts,tsx}": "eslint --fix"
  }
}
