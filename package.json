{
  "name": "time-tracker",
  "shortName": "time-tracker",
  "version": "1.0.0",
  "description": "time-tracker-app",
  "themeColor": "#000000",
  "scripts": {
    "start": "node --experimental-json-modules --no-warnings ./compiler/scripts/dev-server.js",
    "build": "node --experimental-json-modules --no-warnings ./compiler/scripts/build.js",
    "stylelint": "stylelint './src/interface/**/*.css' -f verbose",
    "stylelint-fix": "stylelint './src/interface/**/*.css' -f verbose --fix",
    "eslint": "eslint ./src/**/*.{ts,tsx}",
    "eslint-fix": "eslint ./src/**/*.{ts,tsx} --fix",
    "eslint-all": "eslint ./{compiler,src}/**/*.{cjs,js,jsx,ts,tsx} --no-error-on-unmatched-pattern --quiet",
    "analyze": "webpack-bundle-analyzer build/stats.json"
  },
  "keywords": [
    "time-tracker"
  ],
  "author": "Semakov Andrey",
  "type": "module",
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-hot-loader": "^4.1.1",
    "@types/uuid": "^8.3.0",
    "autoprefixer": "^10.2.6",
    "camelcase": "^5.3.1",
    "css-loader": "^5.2.6",
    "del": "^6.0.0",
    "eazy-logger": "^3.1.0",
    "esbuild-loader": "^2.13.1",
    "fast-glob": "^3.2.5",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "html-webpack-plugin": "^5.3.2",
    "image-size": "^1.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "normalize.css": "^8.0.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-loader": "^6.1.0",
    "postcss-modules": "^4.1.3",
    "postcss-nested": "^5.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "redux": "^4.1.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "typescript": "^4.3.2",
    "uuid": "^3.4.0",
    "webpack": "^5.38.1",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "chokidar": "^2.1.8",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "^7.28.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-rulesdir": "^0.2.0",
    "express": "^4.17.1",
    "log-update": "^4.0.0",
    "module-alias": "^2.2.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-order": "^4.1.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0"
  },
  "peerDependencies": {
    "postcss": "^8.3.5"
  },
  "config": {
    "devPort": 3000,
    "entries": {
      "index": "index"
    },
    "directories": {
      "source": "src",
      "development": "tmp",
      "production": "build",
      "assets": "assets"
    }
  },
  "browserslist": [
    "> 0.5%"
  ],
  "_moduleAliases": {
    "eslint-plugin-rulesdir": "./compiler/eslint/rulesdir.cjs"
  }
}
